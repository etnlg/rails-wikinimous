<div class = "container border bg-light">
  <div class="w-100 row justify-content-between mt-3">
      <div class="col-4">
        <p><a class = "text-secondary text-decoration-none" href="/articles">Wiki</a></p>
      </div>
      <div class="col-4">
        <p><a class = "text-secondary text-decoration-none" href="/articles/new">New article</a></p>
      </div>
  </div>
</div>

<%= form_with(model: @article) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
 <% end %>
